  <div id="main">
    <h1 class="slogan1">Feedback</h1>
     
      <% @feedback.each do |feedback| %>
      <div class="feedback">
        <p><strong><%= feedback.title %></strong></p>
        <p><%= feedback.text %></p>
        <button type="button" class="btn"><%= link_to 'Show', feedback_path(feedback) %></button>
        <button type="button" class="btn"><%= link_to 'Edit' , edit_feedback_path(feedback) %></button>
        <button type="button" class="btn"><%= link_to 'Destroy',
                                    feedback_path(feedback),
                                    method: :delete,
                                    data: { confirm: 'Are you sure?' } %>
        </button>
      </div>
      <% end %>
    <%= link_to 'Back to home page', controller: "home", action: "index" %>
  
</div>

