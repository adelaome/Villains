<div class=bar>
    <h1 id=header_title>
        villains
    </h1>
    <nav>
    	<ul class="topnav" id="myTopnav">
    		<li><a  href="/">Home</a></li>
    		<li><a id="active" href= "/feedback/new">FeedBack</a></li>
    		<li><a href="/about_us">About Us</a></li>
